# ===== CONFIGURAÇÕES DE BANCO =====
# URL do banco PostgreSQL (produção)
DATABASE_URL=postgresql://usuario:senha@host:5432/banco

# ===== REDIS =====
# URL do Redis (produção)
REDIS_URL=redis://usuario:senha@host:6379

# ===== SEGURANÇA =====
# Chave secreta para JWT (MUDE EM PRODUÇÃO!)
SECRET_KEY=sua-chave-super-secreta-aqui

# ===== TWILIO =====
# Credenciais do Twilio
TWILIO_ACCOUNT_SID=seu-account-sid
TWILIO_AUTH_TOKEN=seu-auth-token

# ===== OPENAI =====
# Chave da API OpenAI
OPENAI_API_KEY=sua-chave-openai

# ===== GOOGLE =====
# Credenciais do Google (para Sheets, Calendar)
GOOGLE_CREDENTIALS=seu-json-do-google

# ===== CHATWOOT =====
# Configurações do Chatwoot
CHATWOOT_API_KEY=sua-chave-chatwoot
CHATWOOT_BASE_URL=https://seu-chatwoot.com 